<section class="mt-5">
    <h1 class="text-danger"> Historial</h1>
        <% if ( productos.length === 0) { %> 
            <h2 class="bg-danger rounded text-center m-4">No se encontraron datos</h2>
        <% } else {%> 
    <table class="table table-dark">
        <tr class="text-warning">
            <th>Producto</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
        <% productos.forEach(function(user) { %> 
        <tr>
            <td><%= user.prod %></td>
            <td><%= user.price %></td>
            <td><img id="imagenPrevisualizacion" src="<%= user.img%>"  style="width: 3rem;"></td>
        </tr>
        <% }) %> 
    </table>
    <% } %> 
</section>